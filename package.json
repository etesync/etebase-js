    "@types/jest": "^24.0.22",
    "@types/node": "^11.9.3",
    "@types/node-rsa": "^1.0.0",
    "@types/sjcl": "^1.0.28",
    "@types/urijs": "^1.15.38",
    "@types/uuid": "^3.4.3",
